{{#partial "title"}}
    {{alerta.tipo}} en {{alerta.heladera}}
{{/partial}}
{{#partial "links"}}
    <link rel="stylesheet" href="/css/incidentes/alerta_detalle.css"/>
    <link
            href="https://api.mapbox.com/mapbox-gl-js/v2.10.0/mapbox-gl.css"
            rel="stylesheet"
    />
{{/partial}}
{{#partial "scripts"}}
    <script src="https://api.mapbox.com/mapbox-gl-js/v2.10.0/mapbox-gl.js"></script>
    <script src="/js/map.js"></script>
{{/partial}}
{{#partial "main-content"}}
    <div class="container">
        <div class="header">
            <h2 class="title"> {{alerta.tipo}} en {{alerta.heladera}}</h2>
            {{#if puedeResolver}}
                <a href="/visitas-tecnicas/new?incidenteId={{alerta.id}}" class="register-button">
                Resolver </a>
            {{/if}}
        </div>
    </div>
{{/partial}}
{{#partial "side-panel"}}
    <div id="map"></div>
{{/partial}}
{{> templates/base}}